{
	"C_Cpp.inlayHints.autoDeclarationTypes.enabled": true,
	"astyle.astylerc": null,
	"[cpp]": {
		"editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd"
	},
	"C_Cpp.inlayHints.parameterNames.enabled": true,
	"editor.fontFamily": "'Source Code Pro', 'monospace', monospace",
	"telemetry.telemetryLevel": "off",
	"C_Cpp.inlayHints.parameterNames.hideLeadingUnderscores": false,
	"C_Cpp.inlayHints.referenceOperator.enabled": true,
	"workbench.preferredDarkColorTheme": "Gruvbox Dark Hard",
	"workbench.preferredLightColorTheme": "Gruvbox Light Soft",
	"editor.renderWhitespace": "all",
	"vsicons.dontShowNewVersionMessage": true,
	"cmake.configureOnOpen": false,
	"mesonbuild.buildFolder": ".vscode-build",
	"editor.inlayHints.fontSize": 11,
	"editor.codeActionsOnSave": {},
	"editor.minimap.enabled": false,
	"debug.allowBreakpointsEverywhere": true,
	"workbench.startupEditor": "none",
	"workbench.colorTheme": "Gruvbox Dark Hard",
	"C_Cpp.autocompleteAddParentheses": true,
	"qttools.loglevel": "warning",
	"C_Cpp.intelliSenseEngineFallback": "enabled",
	"cmake.buildDirectory": "${workspaceFolder}/.build/${buildKit}/${buildType}",
	"C_Cpp.intelliSenseEngine": "disabled",
	"C_Cpp.codeAnalysis.clangTidy.enabled": true,
	"clangd.arguments": [
		"--pretty",
		"--background-index",
		"--header-insertion=never"
	],
	"cmake.copyCompileCommands": "${workspaceFolder}/compile_commands.json",
	"C_Cpp.codeAnalysis.clangTidy.checks.enabled": [
		"bugprone-*",
		"llvm-include-order",
		"performance-*",
		"misc-unused-*",
		"cppcoreguidelines-*",
		"modernize-*",
		"clang-analyzer-*"
	],
	"editor.wordWrapColumn": 100,
	"editor.rulers": [
		100,
	],
	"C_Cpp.clang_format_fallbackStyle": "Microsoft",
	"editor.detectIndentation": false,
	"editor.insertSpaces": false,
	"astyle.c.enable": false,
	"C_Cpp.vcFormat.indent.gotoLabels": "leftmostColumn",
	"editor.indentSize": "tabSize",
	"astyle.cpp.enable": false,
	"rust-analyzer.cargo.buildScripts.invocationLocation": "root",
	"window.menuBarVisibility": "toggle",
	"editor.guides.bracketPairs": "active",
	"indentRainbow.indicatorStyle": "light",
	"window.titleBarStyle": "custom",
	"window.title": "${dirty}${activeEditorShort}${separator}${rootName}${separator}${profileName}",
	"terminal.integrated.fontFamily": "Inconsolata",
	"workbench.iconTheme": "vscode-icons",
	"editor.fontSize": 13,
	"explorer.confirmDelete": false,
	"git.alwaysSignOff": true,
	"files.autoGuessEncoding": true,
	"editor.formatOnSave": true
}